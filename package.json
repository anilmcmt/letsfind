{
  "name": "letsfind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:dev": "env-cmd -f env/.env.development next build",
    "start:dev": "env-cmd -f env/.env.development next start",
    "build:uat": "env-cmd -f env/.env.uat next build",
    "start:uat": "env-cmd -f env/.env.uat next start",
    "build:prod": "env-cmd -f env/.env.production next build",
    "start:prod": "env-cmd -f env/.env.production next start"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "next": "^14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.2.3"
  }
}
